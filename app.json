{
  "expo": {
    "name": "rn-assignment-video",
    "slug": "rn-assignment-video",
    "version": "1.0.0",
    "icon": "./app/assets/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#8D99AE"
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/app-icon.png",
        "backgroundColor": "#8D99AE"
      },
      "package": "pl.ekrzeptowski.rn_assignment_video"
    },
    "plugins": ["expo-secure-store", "react-native-video", [
      "expo-screen-orientation",
      {
        "initialOrientation": "PORTRAIT"
      }
    ]]
  }
}
